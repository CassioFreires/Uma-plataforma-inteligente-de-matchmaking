  "main": "server.js",
  "scripts": {
    "start": "tsc && node ./dist/server.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon ./dist/server.js\"",
    "test": "jest",
    "migration:dev": "npx knex migrate:latest --env user_test",
